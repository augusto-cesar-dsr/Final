
<% @questions.each do |question| %>
  <div class="content ">
    <div class="content ">
      <h4 class="content ">
        <div class="content " >
          <%= question.description %>
        </div>
      </h4>
      <div class="content">
        <div class="content">
          <ul>
            <% question.answers.each do |answer| %>
              <li>
                <%= answer.description %>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
<% end %>